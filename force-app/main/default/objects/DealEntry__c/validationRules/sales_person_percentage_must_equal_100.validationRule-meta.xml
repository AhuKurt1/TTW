<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>sales_person_percentage_must_equal_100</fullName>
    <active>true</active>
    <errorConditionFormula>(Primary_Sales_Person_Percentage__c  +   Secondary_Sales_Person_Percentage__c ) &lt;&gt; 1</errorConditionFormula>
    <errorMessage>The total sales person percentage must add up to 100%.</errorMessage>
</ValidationRule>
