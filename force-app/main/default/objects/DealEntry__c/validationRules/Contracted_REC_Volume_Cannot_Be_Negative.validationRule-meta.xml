<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contracted_REC_Volume_Cannot_Be_Negative</fullName>
    <active>true</active>
    <description>If Contracted Volume is less than 0</description>
    <errorConditionFormula>Contracted_REC_Volume__c &lt; 0</errorConditionFormula>
    <errorDisplayField>Contracted_REC_Volume__c</errorDisplayField>
    <errorMessage>Contracted REC Volume cannot be negative.</errorMessage>
</ValidationRule>
